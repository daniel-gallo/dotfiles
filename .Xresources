! URxvt options
! Enable extensions
URXvt.perl-ext-common:       default, clipboard, tabbed, matcher, font-size
! Customize scrollbar
URxvt*scrollBar:             true
URxvt*thickness:             5
URxvt*scrollColor:           #1E1E1E
URxvt*scrollBar_right:       true
URxvt*scrollstyle:           plain
! Underline URLs
URXvt.underlineURLs:         true
URxvt.urlLauncher:           firefox
URxvt.matcher.button:        1
URxvt.matcher.pattern.1:     \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
URxvt.matcher.pattern.2:     \\B(/\\S+?):(\\d+)(?=:|$)
! Resize font using shortcuts
URxvt.keysym.C-Up:           font-size:increase
URxvt.keysym.C-Down:         font-size:decrease
URxvt.keysym.C-S-Up:         font-size:incglobal
URxvt.keysym.C-S-Down:       font-size:decglobal
URxvt.keysym.C-equal:        font-size:reset
URxvt.keysym.C-slash:        font-size:show
! Regular copy and paste
URxvt.keysym.Shift-Control-C:eval:selection_to_clipboard
URxvt.keysym.Shift-Control-V:eval:paste_clipboard
URxvt.iso14755:              false
URxvt.iso14755_52:           false
! Customize font
URxvt.font:                  xft:DejaVu Sans Mono for Powerline:size=11:antialiasing=True

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
